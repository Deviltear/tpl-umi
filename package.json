{
	"name": "tpl-umifile",
	"displayName": "Tpl-umifile",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tpl-umifile.creatPage",
		"onCommand:tpl-umifile.creatSelect",
		"onCommand:tpl-umifile.creatComponent"

	],
  "main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tpl-umifile.creatPage",
				"title": "tpl-create-pages"
			},
      {
				"command": "tpl-umifile.creatComponent",
				"title": "tpl-create-Components"
			},
      {
				"command": "tpl-umifile.creatSelect",
				"title": "tpl-help-select"
			}

		],
		"keybindings": [
      {
        "command": "tpl-umifile.creatSelect",
        "key": "ctrl+f10",
        "mac": "cmd+f10"
      },
      {
        "command": "tpl-umifile.creatPage",
        "key": "ctrl+f11",
        "mac": "cmd+f11"
      }
    ],
		"menus": {
      "editor/context": [
        {
          "command": "tpl-umifile.creatSelect",
          "group": "1_find-test-file",
          "when": "explorerResourceIsFolder"
				}
      ],
			"editor/title": [
        {
          "command": "find-test-file.jumpToTest",
          "group": "navigation"
        }

      ]

    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
